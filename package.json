{
  "name": "mini-project-covid-api",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "build": "yarn clean && yarn transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "NODE_ENV=development yarn build && yarn server",
    "prod": "NODE_ENV=production yarn build && yarn server",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "axios": "^0.26.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.21.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {}
}
